<%- include("partials/header") -%>
<h1>Home</h1>
<p> <%= html_homeStartingContent  %> </p>

<% html_posts.forEach(function(post)  { %>

    <h1><%=post.title%></h1>
    <% if (post.body.length >= 100) {%>
      <p><%=post.body.substring(0, 100) + " ..."%></p>
    <%} else { %>%>

      <p><%=post.body%></p>
    <%} %>
    <a href="/posts/+<%=post.title%>">Read More</a>

<% }); %>

      <%- include("partials/footer") -%>
